    skos:inScheme iss-sdcr:DirectCauseReportedTypes ;
    skos:notation "B.1.2.1"^^xsd:token ;
    skos:prefLabel "B.1.2.1-Signal passed at danger with passing of a danger point"@en .

<http://data.europa.eu/949/iss/concepts/safety-direct-cause-reported-types/B.1.2.10>
    rdf:type skos:Concept ;
    skos:broader <http://data.europa.eu/949/iss/concepts/safety-direct-cause-reported-types/B.1.2> ;
    skos:definition "Any occasion when a passenger train is stopped in a tunnel for more than 10 minutes. Stops in underground stations should be excluded."@en ;
    skos:inScheme iss-sdcr:DirectCauseReportedTypes ;
    skos:notation "B.1.2.10"^^xsd:token ;
    skos:prefLabel "B.1.2.10-Long stop in tunnel"@en .

<http://data.europa.eu/949/iss/concepts/safety-direct-cause-reported-types/B.1.2.11>
    rdf:type skos:Concept ;
    skos:broader <http://data.europa.eu/949/iss/concepts/safety-direct-cause-reported-types/B.1.2> ;
    skos:definition "Any situation in which a brake application by the driver is exceeding applicable limits"@en ;
    skos:inScheme iss-sdcr:DirectCauseReportedTypes ;
    skos:notation "B.1.2.11"^^xsd:token ;
