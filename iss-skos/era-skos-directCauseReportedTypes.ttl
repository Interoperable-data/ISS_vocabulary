<http://data.europa.eu/949/iss/concepts/safety-direct-cause-reported-types/B.1.2.12>
    rdf:type skos:Concept ;
    skos:broader <http://data.europa.eu/949/iss/concepts/safety-direct-cause-reported-types/B.1.2> ;
    skos:definition "Any situation in which the brake system is not set correctly in relation with the load"@en ;
