iss-set:SafetyEventTypes a skos:ConceptScheme ; 
    dct:issued "2024-04-04"^^xsd:date ;
    dct:modified "2024-08-29"^^xsd:date ;
    dct:modified "2024-09-03"^^xsd:date ;
    cc:license <https://creativecommons.org/licenses/by/4.0/> ;
    rdfs:comment "Controlled SKOS-based vocabulary defined by the European Union Agency for Railways to describe concepts related to the Information Sharing System (ISS) in accordance with the draft delegated act of the future CSM ASLP Regulation."@en ;
    rdfs:label "Safety Event Types"@en ;
    skos:prefLabel "Safety Event Types"@en ;
    dct:title "Concept scheme grouping safety event types"@en .



#################################################################
#
#    Concept instances
#
#################################################################


iss-set:A
        rdf:type        skos:Concept ;         skos:inScheme iss-set:SafetyEventTypes;         skos:topConceptOf iss-set:SafetyEventTypes;
        skos:prefLabel  "Category A Event"@en .
        

iss-set:B
        rdf:type        skos:Concept ;         skos:inScheme iss-set:SafetyEventTypes;         skos:topConceptOf iss-set:SafetyEventTypes;
        skos:prefLabel  "Category B Event"@en .
        
iss-set:C
        rdf:type        skos:Concept ;         skos:inScheme iss-set:SafetyEventTypes;         skos:topConceptOf iss-set:SafetyEventTypes;
        skos:prefLabel  "Category C Event"@en .
   
iss-set:A-0 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition "Any accident other than a collision; derailment; level crossing accident; an accident to person involving rolling stock in motion or a fire in rolling stock; falling within the A.0 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other accident"@en .

iss-set:A-0-0 a skos:Concept;
  skos:broader iss-set:A0;
  skos:definition "Any other accident falling within the category �other accident�; but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A0.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other"@en .

iss-set:A-0-1 a skos:Concept;
  skos:broader iss-set:A0;
  skos:definition "Any accident caused by electric current passing through the body."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A0.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Electric shock"@en .

iss-set:A-0-2 a skos:Concept;
  skos:broader iss-set:A0;
  skos:definition "Any accident caused by cargo/freight falling from a height; including during loading and unloading operations."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A0.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Cargo/freight falling from a height"@en .

iss-set:A-0-3 a skos:Concept;
  skos:broader iss-set:A0;
  skos:definition "Any accident involving dangerous goods to be reported in accordance with section 1.8.5 of �RID� (as referred to in Annex II.1 to Directive 2008/68/EC; as amended) and not covered by another type A event."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A0.3"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Dangerous goods accident not related to another type A event"@en .

iss-set:A-1 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition "A collision event falling within the A.1 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Collisions"@en .

iss-set:A-1-0 a skos:Concept;
  skos:broader iss-set:A1;
  skos:definition "Any other accident falling within the category �collisions�; but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A1.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other collision type"@en .

iss-set:A-1-1 a skos:Concept;
  skos:broader iss-set:A1;
  skos:definition "A front to front; front to end or a side collision between a part of a train and a part of another train or rail vehicle; or with shunting rolling stock."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A1.1 "^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Collision of train with a train/rail vehicle"@en .

iss-set:A1-2 a skos:Concept;
  skos:broader iss-set:A1;
  skos:definition "A collision between a part of a train and objects fixed or temporarily present on or near the track (except at level crossings if lost by a crossing vehicle or user); including collision with overhead contact lines."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A1.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Collision of train with obstacle within the clearance gauge"@en .

iss-set:A1-3 a skos:Concept;
  skos:broader iss-set:A1;
  skos:definition "Same as A1.1 but concerning more rail vehicles not forming a train."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A1.3 "^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Collision of one or more rail vehicles with another rail vehicle"@en .

iss-set:A1-4 a skos:Concept;
  skos:broader iss-set:A1;
  skos:definition "Same as A1.2 but concerning one or more rail vehicles not forming a train."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A1.4 "^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Collision of one or more rail vehicles with obstacle within the clearance gauge"@en .

iss-set:A-2 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition "A derailment event falling within the A.2 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Derailments"@en .

iss-set:A2-0 a skos:Concept;
  skos:broader iss-set:A2;
  skos:definition "Any other accident falling within the category �derailments�; but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A2.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other derailment type"@en .

iss-set:A2-1 a skos:Concept;
  skos:broader iss-set:A2;
  skos:definition "Any case in which at least one wheel of a train leaves the rails."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A2.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Derailment of train"@en .

iss-set:A-2-2 a skos:Concept;
  skos:broader iss-set:A2;
  skos:definition "Same as A2.1 but concerning one or more rail vehicles not forming a train."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A2.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Derailment of one or more rail vehicles"@en .

iss-set:A-3 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition " A level crossing accident falling within the A.3 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A3"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Level Crossing Accident"@en .

iss-set:A-3-0 a skos:Concept;
  skos:broader iss-set:A3;
  skos:definition "Any other accident falling within the category �level crossing accident�; but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A3.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other level crossing accident"@en .

iss-set:A-3-1 a skos:Concept;
  skos:broader iss-set:A3;
  skos:definition " A level crossing accident involving a train and falling within the A.3.1 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A3.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Level Crossing Accident involving a train"@en .

iss-set:A-3-1-1 a skos:Concept;
  skos:broader iss-set:A3-1;
  skos:definition "Any accident at level crossings involving at least one train and crossing pedestrians. Note: pedestrians also include people crossing with bicycles (or other similar small vehicles normally allowed on pedestrian/bike paths)."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A3.1.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Level Crossing Accident involving a train and pedestrians"@en .

iss-set:A-3-1-2 a skos:Concept;
  skos:broader iss-set:A3-1;
  skos:definition "Any accident at level crossings involving at least one train and one or more crossing vehicles or objects temporarily present on or near the track if lost by a crossing vehicle or user."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A3.1.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Level Crossing Accident involving a train and crossing vehicles"@en .

iss-set:A3-2 a skos:Concept;
  skos:broader iss-set:A3;
  skos:definition "Any accident at level crossings involving one or more rail vehicles and one or more crossing vehicles; crossing users such as pedestrians or objects temporarily present on or near the track if lost by a crossing vehicle or user."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A3.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Level Crossing Accident involving one or more rail vehicles"@en .

iss-set:A-4 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition " An accident to persons involving rolling stock in motion falling within the A.4 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A4"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Accidents to persons involving rolling stock in motion"@en .

iss-set:A-4-0 a skos:Concept;
  skos:broader iss-set:A4;
  skos:definition "Any other accident falling within the category �accidents to persons involving rolling stock in motion�; but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A4.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other accidents to persons involving rolling stock in motion"@en .

iss-set:A-4-1 a skos:Concept;
  skos:broader iss-set:A4;
  skos:definition "Accidents to one or more persons who are either hit by a train or by an object attached to; or that has become detached from the train; this includes persons who fall from trains as well as persons who fall or are hit by loose objects when travelling on board trains."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A4.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Accidents to persons involving a train in motion"@en .

iss-set:A-4-2 a skos:Concept;
  skos:broader iss-set:A4;
  skos:definition "Accidents to one or more persons who are either hit by a railway vehicle or by an object attached to; or that has become detached from the vehicle; this includes persons who fall from railway vehicles as well as persons who fall or are hit by loose objects when travelling on board vehicles."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A4.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Accidents to persons involving rail vehicle in motion"@en .

iss-set:A-5 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition "A fire (or explosion) in rolling stock (including its load) event falling within the A.5 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A5"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Fire (or explosion) in rolling stock"@en .

iss-set:A-5-0 a skos:Concept;
  skos:broader iss-set:A5;
  skos:definition "Any other accident falling within the category �fire (or explosion) in rolling stock� (including its load); but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A5.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Others fire (or explosion) in rolling stock"@en .

iss-set:A-5-1 a skos:Concept;
  skos:broader iss-set:A5;
  skos:definition "A fire or explosion that occurs in a train (including its load)."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A5.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Fire (or explosion) in a train"@en .

iss-set:A-5-2 a skos:Concept;
  skos:broader iss-set:A5;
  skos:definition "A fire or explosion that occurs in a rail vehicle (including its load)."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A5.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Fire (or explosion) involving one or more rail vehicles"@en .

iss-set:A-6 a skos:Concept;
  skos:broader iss-set:A;
  skos:definition "A suicide or attempted suicide falling within the A.6 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A6"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Suicides and attempted suicides (voluntary reporting)"@en .

iss-set:A-6-1 a skos:Concept;
  skos:broader iss-set:A6;
  skos:definition "An act to deliberately injure oneself resulting in death; as recorded and classified by the competent national authority."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A6.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Suicide (voluntary reporting)"@en .

iss-set:A-6-2 a skos:Concept;
  skos:broader iss-set:A6;
  skos:definition "An act to deliberately injure oneself resulting in serious injury; as recorded and classified by the competent national authority."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "A6.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Attempted suicide (voluntary reporting)"@en .

iss-set:B-0 a skos:Concept;
  skos:broader iss-set:B;
  skos:definition " Other category B event falling within the B.0 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other category B event types"@en .

iss-set:B-0-0 a skos:Concept;
  skos:broader iss-set:B-0;
  skos:definition "Any other category B event type not covered by any of the category or sub-categories coded above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other  un-coded category B event types"@en .

iss-set:B-0-1 a skos:Concept;
  skos:broader iss-set:B-0;
  skos:definition "Any occasion in which a fire is taking place in proximity of rail infrastructure, affecting the integrity of the infrastructure or the operations."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Fire in proximity of rail infrastructure"@en .

iss-set:B-0-2 a skos:Concept;
  skos:broader iss-set:B-0;
  skos:definition "Any occasion in which the unauthorised presence of staff/employees (i.e. not in accordance with the applicable requirements) is detected. This also includes sub-contractor staff."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Unauthorised presence of staff/employees on railway system"@en .

iss-set:B-0-3 a skos:Concept;
  skos:broader iss-set:B-0;
  skos:definition "Any occasion in which the unauthorised presence of third parties on the railway system (i.e. not in accordance with the applicable requirements) is detected."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0.3"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Unauthorised presence of other third parties on the railway system"@en .

iss-set:B-0-4 a skos:Concept;
  skos:broader iss-set:B-0;
  skos:definition "Any occasion in which the improper or incautious use by persons of authorised passages between platforms in the stations is detected"@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0.4"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Improper or incautious use of authorised passages between platforms in the stations"@en .

iss-set:B-0-5 a skos:Concept;
  skos:broader iss-set:B-0;
  skos:definition "Any incident involving dangerous goods to be reported in accordance with section 1.8.5 of �RID� (as referred to in Annex II.1 to Directive 2008/68/EC, as amended) and not covered by another type B event."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.0.5"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Dangerous goods incidents not related to another type B event"@en .

iss-set:B-1 a skos:Concept;
  skos:broader iss-set:B;
  skos:definition "An operation failure event falling within the B.1 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Operation failures"@en .

iss-set:B-1-0 a skos:Concept;
  skos:broader iss-set:B-1;
  skos:definition "Any occasion falling within the category 'operation failures', but not covered by the categories �failure to operate the infrastructure� and �failure to operate a train or rail vehicle�. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other un-coded operation failure"@en .

iss-set:B-1-1 a skos:Concept;
  skos:broader iss-set:B-1;
  skos:definition "A failure to operate the infrastructure falling within the B.1.1 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Failure to operate the infrastructure"@en .

iss-set:B-1-1-0 a skos:Concept;
  skos:broader iss-set:B-1-1;
  skos:definition "Any occasion falling within the category �failure to operate the infrastructure�, but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply. "@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.1.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other failure to operate the infrastructure"@en .

iss-set:B-1-1-1 a skos:Concept;
  skos:broader iss-set:B-1-1;
  skos:definition "Any occasion when a train/vehicle is directed on a route that was not planned."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.1.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Improper routing"@en .

iss-set:B-1-1-2 a skos:Concept;
  skos:broader iss-set:B-1-1;
  skos:definition "On track machine(s) or other object(s) used during infrastructure activities positioned outside the authorised area"@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.1.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "On track plant incorrectly outside possession"@en .

iss-set:B-1-1-3 a skos:Concept;
  skos:broader iss-set:B-1-1;
  skos:definition "Any occasion when a switch in a wrong position is run over in trailing direction (converging points) unintentionally."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.1.3"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Pushed switch"@en .

iss-set:B-1-1-4 a skos:Concept;
  skos:broader iss-set:B-1-1;
  skos:definition "Any occasion when a passenger train is stopped in a tunnel for more than 10 minutes. Stops in underground stations should be excluded."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.1.4"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Long stop in tunnel"@en .

iss-set:B-1-2 a skos:Concept;
  skos:broader iss-set:B-1;
  skos:definition "A failure to operate a train or rail vehicle falling within the B.1.2 sub-categories for which detailed information is not (yet) available."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.2"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Failure to operate a train or rail vehicle"@en .

iss-set:B-1-2-0 a skos:Concept;
  skos:broader iss-set:B-1-2;
  skos:definition "Any occasion falling within the category �failure to operate a train or rail vehicle�, but not covered by the subcategories above. A reporting of information in accordance with section 3.3 of Appendix A - Part C shall apply."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.2.0"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Other failure to operate a train or rail vehicle"@en .

iss-set:B-1-2-1 a skos:Concept;
  skos:broader iss-set:B-1-2;
  skos:definition "Any occasion when any part of a train or rail vehicle proceeds beyond its authorised movement and travels beyond the danger point."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.2.1"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Signal passed at danger with passing of a danger point"@en .

iss-set:B-1-2-10 a skos:Concept;
  skos:broader iss-set:B-1-2;
  skos:definition "Any occasion when a passenger train is stopped in a tunnel for more than 10 minutes. Stops in underground stations should be excluded."@en;
  skos:inScheme iss-set:SafetyEventTypes;
  skos:notation "B.1.2.10"^^<http://www.w3.org/2001/XMLSchema#token>;
  skos:prefLabel "Long stop in tunnel"@en .

iss-set:B-1-2-11 a skos:Concept;
  skos:broader iss-set:B-1-2;
  skos:definition "Any situation in which a brake application by the driver is exceeding applicable limits"@en;
